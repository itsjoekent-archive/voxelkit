{
  "name": "@voxelkit/translations",
  "main": "dist/src/index.js",
  "scripts": {
    "compile": "npx ts-node -r tsconfig-paths/register generator.ts && tsc",
    "compile:watch": "npm-run-all --parallel compile:watch:**",
